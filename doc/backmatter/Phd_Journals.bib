@ARTICLE{huth_2004,
  author = {G. Huth and K. Qian},
  title = {{Permanentmagneterregte AC-Servomotoren mit vereinfachten Wicklungssystem}},
  journal = {ETG-Fachberichte},
  year = {2004},
  pages = {15-21},
  volume = {96},
  abstract = {Die fertigungsoptimierte geschraenkte Schale, wegen ihrer Form kurz
	Schmetterling genannt, erfuellt alle Kriterien bei der Nutrastmomentunterdrueckung
	wie eine normale geschraenkte Schale. Wegen fehlender Fuehrungs und
	Anlageflaechen sind bei einer geschraenkten Schale separate Schleifdurchgaenge
	und Spannvorrichtungen fuer das Auf- und Umspannen pro geschliffene
	Seite erforderlich. Die Schleifkosten steigen auf das Mehrfache gegenueber
	einer geraden Schale, welche im Durchlauf geschliffen wird. Der Schmetterling
	wurde entwickelt, um die Fertigungskosten auf das Niveau der geraden
